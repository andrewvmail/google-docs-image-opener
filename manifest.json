{
  "manifest_version": 3,
  "name": "Google Docs Image Opener Extension",
  "description": "Open images in Google Docs in a new tab",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "filesystem"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://docs.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  }
}